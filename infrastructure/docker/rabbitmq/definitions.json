{
  "rabbit_version": "3.12.0",
  "rabbitmq_version": "3.12.0",
  "product_name": "RabbitMQ",
  "product_version": "3.12.0",
  "users": [
    {
      "name": "admin",
      "password": "admin123",
      "tags": ["administrator"],
      "limits": {}
    },
    {
      "name": "afripay_user",
      "password": "afripay123",
      "tags": [],
      "limits": {}
    }
  ],
  "vhosts": [
    {
      "name": "/",
      "description": "Default virtual host",
      "tags": [],
      "default_queue_type": "classic"
    },
    {
      "name": "afripay",
      "description": "AfriPay Universal virtual host",
      "tags": [],
      "default_queue_type": "classic"
    }
  ],
  "permissions": [
    {
      "user": "admin",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    },
    {
      "user": "admin",
      "vhost": "afripay",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    },
    {
      "user": "afripay_user",
      "vhost": "afripay",
      "configure": "^afripay\\.",
      "write": "^afripay\\.",
      "read": "^afripay\\."
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "cluster_name",
      "value": "afripay-cluster"
    }
  ],
  "policies": [
    {
      "vhost": "afripay",
      "name": "ha-all",
      "pattern": "^afripay\\.",
      "apply-to": "all",
      "definition": {
        "ha-mode": "all",
        "ha-sync-mode": "automatic",
        "message-ttl": 86400000,
        "max-length": 10000
      },
      "priority": 0
    },
    {
      "vhost": "afripay",
      "name": "dlx-policy",
      "pattern": "^afripay\\..*\\.main$",
      "apply-to": "queues",
      "definition": {
        "dead-letter-exchange": "afripay.dlx",
        "dead-letter-routing-key": "failed"
      },
      "priority": 1
    }
  ],
  "queues": [
    {
      "name": "afripay.transactions.pending",
      "vhost": "afripay",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 10000,
        "x-dead-letter-exchange": "afripay.dlx",
        "x-dead-letter-routing-key": "transactions.failed"
      }
    },
    {
      "name": "afripay.transactions.processing",
      "vhost": "afripay",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 1800000,
        "x-max-length": 5000,
        "x-dead-letter-exchange": "afripay.dlx",
        "x-dead-letter-routing-key": "transactions.failed"
      }
    },
    {
      "name": "afripay.transactions.completed",
      "vhost": "afripay",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-max-length": 50000
      }
    },
    {
      "name": "afripay.notifications.sms",
      "vhost": "afripay",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 300000,
        "x-max-length": 10000,
        "x-dead-letter-exchange": "afripay.dlx",
        "x-dead-letter-routing-key": "notifications.failed"
      }
    },
    {
      "name": "afripay.notifications.push",
      "vhost": "afripay",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 300000,
        "x-max-length": 10000,
        "x-dead-letter-exchange": "afripay.dlx",
        "x-dead-letter-routing-key": "notifications.failed"
      }
    },
    {
      "name": "afripay.notifications.email",
      "vhost": "afripay",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 1800000,
        "x-max-length": 5000,
        "x-dead-letter-exchange": "afripay.dlx",
        "x-dead-letter-routing-key": "notifications.failed"
      }
    },
    {
      "name": "afripay.biometric.enrollment",
      "vhost": "afripay",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 600000,
        "x-max-length": 1000
      }
    },
    {
      "name": "afripay.biometric.verification",
      "vhost": "afripay",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 300000,
        "x-max-length": 5000
      }
    },
    {
      "name": "afripay.agents.terminal.events",
      "vhost": "afripay",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 10000
      }
    },
    {
      "name": "afripay.analytics.events",
      "vhost": "afripay",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-max-length": 100000
      }
    },
    {
      "name": "afripay.audit.logs",
      "vhost": "afripay",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 604800000,
        "x-max-length": 500000
      }
    },
    {
      "name": "afripay.ussd.sessions",
      "vhost": "afripay",
      "durable": false,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 300000,
        "x-max-length": 10000
      }
    },
    {
      "name": "afripay.fraud.detection",
      "vhost": "afripay",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 1800000,
        "x-max-length": 20000,
        "x-dead-letter-exchange": "afripay.dlx",
        "x-dead-letter-routing-key": "fraud.failed"
      }
    },
    {
      "name": "afripay.dlq.failed",
      "vhost": "afripay",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 604800000,
        "x-max-length": 50000
      }
    }
  ],
  "exchanges": [
    {
      "name": "afripay.transactions",
      "vhost": "afripay",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "afripay.notifications",
      "vhost": "afripay",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "afripay.biometric",
      "vhost": "afripay",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "afripay.agents",
      "vhost": "afripay",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "afripay.analytics",
      "vhost": "afripay",
      "type": "fanout",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "afripay.audit",
      "vhost": "afripay",
      "type": "fanout",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "afripay.ussd",
      "vhost": "afripay",
      "type": "direct",
      "durable": false,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "afripay.fraud",
      "vhost": "afripay",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "afripay.dlx",
      "vhost": "afripay",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "afripay.transactions",
      "vhost": "afripay",
      "destination": "afripay.transactions.pending",
      "destination_type": "queue",
      "routing_key": "transaction.created",
      "arguments": {}
    },
    {
      "source": "afripay.transactions",
      "vhost": "afripay",
      "destination": "afripay.transactions.processing",
      "destination_type": "queue",
      "routing_key": "transaction.processing",
      "arguments": {}
    },
    {
      "source": "afripay.transactions",
      "vhost": "afripay",
      "destination": "afripay.transactions.completed",
      "destination_type": "queue",
      "routing_key": "transaction.completed",
      "arguments": {}
    },
    {
      "source": "afripay.transactions",
      "vhost": "afripay",
      "destination": "afripay.analytics",
      "destination_type": "exchange",
      "routing_key": "transaction.*",
      "arguments": {}
    },
    {
      "source": "afripay.notifications",
      "vhost": "afripay",
      "destination": "afripay.notifications.sms",
      "destination_type": "queue",
      "routing_key": "sms",
      "arguments": {}
    },
    {
      "source": "afripay.notifications",
      "vhost": "afripay",
      "destination": "afripay.notifications.push",
      "destination_type": "queue",
      "routing_key": "push",
      "arguments": {}
    },
    {
      "source": "afripay.notifications",
      "vhost": "afripay",
      "destination": "afripay.notifications.email",
      "destination_type": "queue",
      "routing_key": "email",
      "arguments": {}
    },
    {
      "source": "afripay.biometric",
      "vhost": "afripay",
      "destination": "afripay.biometric.enrollment",
      "destination_type": "queue",
      "routing_key": "enrollment.*",
      "arguments": {}
    },
    {
      "source": "afripay.biometric",
      "vhost": "afripay",
      "destination": "afripay.biometric.verification",
      "destination_type": "queue",
      "routing_key": "verification.*",
      "arguments": {}
    },
    {
      "source": "afripay.agents",
      "vhost": "afripay",
      "destination": "afripay.agents.terminal.events",
      "destination_type": "queue",
      "routing_key": "terminal.*",
      "arguments": {}
    },
    {
      "source": "afripay.analytics",
      "vhost": "afripay",
      "destination": "afripay.analytics.events",
      "destination_type": "queue",
      "routing_key": "",
      "arguments": {}
    },
    {
      "source": "afripay.audit",
      "vhost": "afripay",
      "destination": "afripay.audit.logs",
      "destination_type": "queue",
      "routing_key": "",
      "arguments": {}
    },
    {
      "source": "afripay.ussd",
      "vhost": "afripay",
      "destination": "afripay.ussd.sessions",
      "destination_type": "queue",
      "routing_key": "session",
      "arguments": {}
    },
    {
      "source": "afripay.fraud",
      "vhost": "afripay",
      "destination": "afripay.fraud.detection",
      "destination_type": "queue",
      "routing_key": "transaction.check",
      "arguments": {}
    },
    {
      "source": "afripay.dlx",
      "vhost": "afripay",
